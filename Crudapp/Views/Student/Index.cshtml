@model System.Data.DataTable

@{
    ViewData["Title"] = "List Of Students";
}
<div class="jumbotron">
    <h1 class="text-center">@ViewData["Title"]</h1>
</div>



<table class="table table-bordered ">
    <thead class="thead-light">
        <tr>
            <th>
                Student Roll NO
            </th>
            <th>
                Student Name
            </th>

            <th>
                Age
            </th>
            <th>
                <a class="btn btn-primary" asp-area="" asp-controller="Student" asp-action="AddStudent">Add Student</a>
            </th>
        </tr>
    </thead>
    <tbody>
        @for (int i = 0; i < Model.Rows.Count; i++)
        {

        <tr>
            <td>
                @Model.Rows[i]["SRollno"]
            </td>
            <td>
                @Model.Rows[i]["SName"]
            </td>

            <td>
                @Model.Rows[i]["Age"]
            </td>
           
        </tr>
        }
    </tbody>
</table>
